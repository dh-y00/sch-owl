{
  "properties": [
    {
      "name": "sch.owl.server.url",
      "type": "java.lang.String",
      "description": "权限服务的跟路径."
    },
    {
      "name": "sch.owl.server.logout-url",
      "type": "java.lang.String",
      "description": "触发登录退出的接口.",
