      "defaultValue": "/inner/remote/getDeptIdAndChildDeptId"
    },
    {
      "name": "sch.owl.server.dept-id-by-role-url",
      "type": "java.lang.String",
      "description": "根据角色获取部门id.",
      "defaultValue": "/inner/remote/getDeptIdByRole"
    },
    {
      "name": "sch.owl.server.post-code-by-user-id-url",
      "type": "java.lang.String",
      "description": "根据 user ID 获取岗位.",
      "defaultValue": "/inner/remote/getPostCodeByUserId"
    },
    {
