      "defaultValue": "/inner/sse/connect/create"
    },
    {
      "name": "sch.owl.sse.job.enabled",
      "type": "java.lang.Boolean",
      "description": "是否开启job.",
      "defaultValue": false
    },{
      "name": "sch.owl.sse.job.confirm-job-url",
      "type": "java.lang.String",
