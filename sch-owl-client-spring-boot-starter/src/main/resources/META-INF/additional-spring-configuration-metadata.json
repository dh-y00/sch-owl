      "defaultValue": "/inner/remote/getExtraByDeptId"
    },
    {
      "name": "sch.owl.base.login-authentication-token-verity",
      "type": "java.lang.Class",
      "description": "登录 token 的验证器.",
      "defaultValue": "com.rdrk.sch.owl.config.security.RemoteJwtLoginAuthenticationTokenVerity"
    },
    {
      "name": "sch.owl.base.token-config",
      "type": "java.lang.Class",
      "description": "token 配置处理器.",
      "defaultValue": "com.rdrk.sch.owl.token.RemoteTokenConfig"
    },
    {
      "name": "sch.owl.base.remote-clazz",
      "type": "java.lang.Class",
      "description": "远程权限系统处理器.",
      "defaultValue": "com.rdrk.sch.owl.HttpRemoteUpmsServe"
    },
    {
      "name": "sch.owl.base.ignore",
      "type": "java.lang.String",
      "description": "不做权限校验的配置.",
